diff -Naur fontconfig-2.9.0.orig/conf.d/70-force-bitmaps.conf fontconfig-2.9.0/conf.d/70-force-bitmaps.conf
--- fontconfig-2.9.0.orig/conf.d/70-force-bitmaps.conf	1970-01-01 01:00:00.000000000 +0100
+++ fontconfig-2.9.0/conf.d/70-force-bitmaps.conf	2012-05-06 11:49:00.122859669 +0200
@@ -0,0 +1,20 @@
+<?xml version="1.0"?>
+<!DOCTYPE fontconfig SYSTEM "fonts.dtd">
+<fontconfig>
+<!-- Accept bitmap fonts -->
+ <selectfont>
+  <acceptfont>
+   <pattern>
+     <patelt name="scalable"><bool>false</bool></patelt>
+   </pattern>
+  </acceptfont>
+ </selectfont>
+<!-- Do not use embedded bitmaps in fonts -->
+ <match target="font" >
+  <edit name="embeddedbitmap" mode="assign"><bool>true</bool></edit>
+ </match>
+<!-- Do not substitute truetype fonts for bitmap ones -->
+ <match target="pattern" >
+  <edit name="prefer_outline"><bool>false</bool></edit>
+ </match>
+</fontconfig>
