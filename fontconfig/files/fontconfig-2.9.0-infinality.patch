diff -Naur fontconfig-2.9.0.orig/conf.d/90-embolden.conf fontconfig-2.9.0/conf.d/90-embolden.conf
--- fontconfig-2.9.0.orig/conf.d/90-embolden.conf	1970-01-01 01:00:00.000000000 +0100
+++ fontconfig-2.9.0/conf.d/90-embolden.conf	2012-05-06 11:49:00.124859649 +0200
@@ -0,0 +1,22 @@
+<?xml version="1.0"?>
+<!DOCTYPE fontconfig SYSTEM "fonts.dtd">
+<fontconfig>
+<!-- Prevent bold-ish fonts from being emboldened -->
+ <match target="font">
+  <!-- check to see if the font is just regular -->
+  <test name="weight" compare="more_eq"><const>semibold</const></test>
+  <!-- check to see if the pattern requests bold -->
+  <test target="pattern" name="weight" compare="eq"><const>bold</const></test>
+  <!-- pretend it's bold now -->
+  <edit name="weight" mode="assign"><const>bold</const></edit>
+ </match>
+<!-- Prevent thin-ish fonts from being emboldened -->
+ <match target="font">
+  <!-- check to see if the font is just regular -->
+  <test name="weight" compare="less"><const>book</const></test>
+  <!-- check to see if the pattern requests bold -->
+  <test target="pattern" name="weight" compare="eq"><const>bold</const></test>
+  <!-- pretend it's bold now -->
+  <edit name="weight" mode="assign"><const>bold</const></edit>
+ </match>
+</fontconfig>
diff -Naur fontconfig-2.9.0.orig/conf.d/90-synthetic.conf fontconfig-2.9.0/conf.d/90-synthetic.conf
